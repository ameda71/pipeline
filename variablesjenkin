pipeline{
    agent any
    environment{
        def myString = "Hello World"
        def myNumber = 10
        def myBool = true
    }

    stages {
        stage ("Demo"){
            steps{
                echo 'Greeting : ${myString}'
                echo 'AGe : ${myNumber}'
                echo 'Are you Alive : ${myBool}'
            }
        
        }
    }
}
